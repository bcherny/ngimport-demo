{
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "declaration": false,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "dom",
      "es2015",
      "es2016.array.include"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "pretty": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es5"
  },
  "files": [
    "index.ts",
    "test.ts"
  ]
}